<%= link_to users_dashboards_path, class:"btn btn-default btn-xs" do %>
   <i class="glyphicon glyphicon-chevron-left"></i> Retourner aux dashboards
   <% end %>

<div class="page-header">
   <h1>
      <%= @dashboard.name %>
       <small>
      <%= form_for @dashboard, url: edit_dashboard_path(@dashboard), html: {method: "get", class:"inline-form"}  do |f| %>
      
         <button type="submit" class="btn btn-default btn-xs">
           <span class="glyphicon glyphicon glyphicon-pencil" aria-hidden="true"></span>
         </button> 
         <% end %>
        <%= form_for @dashboard, html: {method: "delete", class:"inline-form"}  do |f| %>
          <button type="submit" class="btn btn-default btn-xs">
            <span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
          </button> 
        <% end %>
      </small>
   </h1>
</div>
<div class="row">
  <%= render 'modules/history/new' %>
  <%= render 'modules/history/show' %>
</div>
 