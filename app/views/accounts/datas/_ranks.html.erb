          <p><%=@file_ranks[:solo][:wins] %> wins - <%=@file_ranks[:solo][:losses] %> losses (<%=@file_ranks[:solo][:ratio] %>%)</p>
        <% else %>
        <p>&thinsp;</p>
        <p>&thinsp;</p>
        <% end %>
      </div>
    </div>   
  </div>
  <div class="col-md-4">
