<%= @debug %>
<div class="row">
  <div class="col-md-4">
    <div class="thumbnail text-center">
      <div class="caption">
        <h3>Team 3v3</h3>     
      </div>
      <% imagename = @file_ranks[:team3][:tier] + @file_ranks[:team3][:division] %>
      <%= image_tag 'tiers/'+imagename+'.png' %>
      <div class="caption">
        <h4><%=@file_ranks[:team3][:tier].capitalize %> <%=@file_ranks[:team3][:division] %></h4>
        <% if @file_ranks[:team3][:division] != "" %>
        <p><%=@file_ranks[:team3][:lp] %> LP</p>
        <p><%=@file_ranks[:team3][:wins] %> wins - <%=@file_ranks[:team3][:losses] %> losses (<%=@file_ranks[:team3][:ratio] %>%)</p>
        <% else %>
        <p>&thinsp;</p>
        <p>&thinsp;</p>
        <% end %>
      </div>
    </div>
  </div>
  <div class="col-md-4">
    <div class="thumbnail text-center">
      <div class="caption">
        <h3>Solo</h3>
      </div>
      <% imagename = @file_ranks[:solo][:tier] + @file_ranks[:solo][:division] %>
